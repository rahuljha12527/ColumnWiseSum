#include <iostream>
using namespace std;

int main() {
	// your code goes here
   int a[1000][1000];
   int m,n, sum=0;
   cin>>m>>n;
   
  
    
   for(int i=0;i<m;i++){
       for(int j=0;j<n;j++){
           cin>>a[i][j];
       }
   }
   
   
 for(int j=0;j<n;j++){
     for(int i=0;i<m;i++){
           sum=sum+a[i][j];
     }
     
      cout<<sum<<" ";
      sum=0;
 }
   
 
	
}
